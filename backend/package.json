    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3",
    "vitest": "^3.2.3"
