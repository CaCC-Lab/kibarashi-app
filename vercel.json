{
  "installCommand": "npm install && npm run build:logic && cd api && npm install",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3"
    },
    "api/**/*.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}